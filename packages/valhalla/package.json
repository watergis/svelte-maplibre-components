{
	"name": "@watergis/svelte-maplibre-valhalla",
	"version": "2.0.1",
	"description": "This is a svelte component to add valhalla control plugin to maplibre.",
	"main": "dist/index.js",
	"files": [
		"dist",
		"package.json",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/watergis/svelte-maplibre-components.git",
		"directory": "packages/valhalla"
	},
	"keywords": [
		"maplibre",
		"maplibre-gl-js",
		"valhalla",
		"isochrone",
		"routing"
	],
	"author": "Jin IGARASHI",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/watergis/svelte-maplibre-components/issues"
	},
	"homepage": "https://github.com/watergis/svelte-maplibre-components/tree/main/packages/valhalla#readme",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@sveltejs/package": "^2.3.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@watergis/svelte-maplibre-menu": "workspace:^",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^4.5.2"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"maplibre-gl": "^3.6.2",
		"svelte-fa": "^3.0.4"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
