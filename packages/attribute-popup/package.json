{
	"name": "@watergis/svelte-maplibre-attribute-popup",
	"version": "2.0.2",
	"description": "This is a svelte component to add attribute popup plugin to maplibre",
	"main": "dist/index.js",
	"files": [
		"dist",
		"package.json",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/watergis/svelte-maplibre-components.git",
		"directory": "packages/attribute-popup"
	},
	"keywords": [
		"maplibre",
		"maplibre-gl-js",
		"attribute",
		"popup"
	],
	"author": "Jin IGARASHI",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/watergis/svelte-maplibre-components/issues"
	},
	"homepage": "https://github.com/watergis/svelte-maplibre-components/tree/main/packages/attribute-popup#readme",
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.7",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.3",
		"@sveltejs/package": "^2.2.0",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte": "^2.32.4",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.64.1",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7"
	},
	"type": "module",
	"dependencies": {
		"maplibre-gl": "^3.2.1"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
