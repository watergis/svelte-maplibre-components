{
	"name": "@watergis/svelte-maplibre-export",
	"version": "0.1.2",
	"description": "This is a svelte component to add export control plugin to maplibre.",
	"main": "package/index.js",
	"files": [
		"package"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/watergis/svelte-maplibre-components.git",
		"directory": "packages/export"
	},
	"keywords": [
		"maplibre",
		"maplibre-gl-js",
		"export"
	],
	"author": "Jin IGARASHI",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/watergis/svelte-maplibre-components/issues"
	},
	"homepage": "https://github.com/watergis/svelte-maplibre-components/tree/main/packages/search#readme",
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@types/fabric": "^4.5.12",
		"@types/file-saver": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@neodrag/svelte": "^1.2.4",
		"fabric": "^5.2.4",
		"file-saver": "^2.0.5",
		"jspdf": "^2.5.1",
		"maplibre-gl": "^2.4.0"
	}
