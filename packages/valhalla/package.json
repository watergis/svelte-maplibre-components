		"globals": "^15.9.0",
		"pmtiles": "^3.0.7",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-preprocess": "^6.0.2",
		"tslib": "^2.7.0",
