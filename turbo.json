{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "svelte.water-gis.com#build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**"],
      "outputs": ["build/**", ".svelte-kit/**"],
      "outputMode": "new-only"
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**"],
      "outputs": ["packages/**", ".svelte-kit/**"],
      "outputMode": "new-only"
    },
    "format": {
      "cache": false,
      "outputs": [],
      "outputMode": "new-only"
    },
    "lint": {
      "outputs": [],
      "outputMode": "new-only"
    },
    "precommit": {
      "dependsOn": ["format", "lint"],
      "outputs": [],
      "outputMode": "new-only"
    },
    "dev": {
      "cache": false,
      "outputMode": "new-only"
    }
  },
  "globalDependencies": [".eslintrc.json", ".prettierrc", "tsconfig.json"]
}
