		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/watergis/svelte-maplibre-components.git",
		"directory": "packages/export"
	},
	"keywords": [
		"maplibre",
		"maplibre-gl-js",
		"export"
	],
	"author": "Jin IGARASHI",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/watergis/svelte-maplibre-components/issues"
	},
	"homepage": "https://github.com/watergis/svelte-maplibre-components/tree/main/packages/export#readme",
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.13.0",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/fabric": "^5.3.9",
		"@types/file-saver": "^2.0.7",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"bulma": "^1.0.2",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^15.14.0",
		"pmtiles": "^4.1.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"sass": "^1.83.0",
		"svelte": "^5.15.0",
		"svelte-check": "^4.1.1",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.1",
		"vite": "^6.0.4"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^7.0.0",
		"@neodrag/svelte": "^2.2.0",
		"jspdf": "^2.5.2",
		"maplibre-gl": "^5.0.0",
		"svelte-fa": "^4.0.3"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
